html
  include includes/header.pug          
  |Choose a deck:
  select(id= "decks")
    each val in decks
      option(value=val.ID) #{val.name}    
  div(id="deckEdit")
  div(id="deckInfo")
  div(id="deckList")   
    ul(id="creatures")
    ul(id="spells")
    ul(id="lands")   
    ul(id="sideboard")
  div(id="totalCards")
          
  script(src="javascript/jquery-3.3.1.js")
  script(src="js/vendor/foundation.js")
  script(src='javascript/script.js')
    
   
      
    
    
